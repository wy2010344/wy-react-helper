{
  "name": "wy-react-native",
  "version": "1.0.1",
  "type": "module",
  "description": "React Native helper utilities for wy framework",
  "keywords": [
    "react-native",
    "mobile",
    "utilities",
    "wy"
  ],
  "author": "wy2010344",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wy2010344/wy-react-helper.git"
  },
  "homepage": "https://github.com/wy2010344/wy-react-helper#readme",
  "bugs": {
    "url": "https://github.com/wy2010344/wy-react-helper/issues"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": ">=1.8.0",
    "react-native-reanimated": ">=2.1.0",
    "wy-helper": "workspace:*",
    "wy-react-helper": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18.3.12",
    "tsdown": "latest"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
